footer.comment { background-color:#dcdcdc; width:360px; height:60px; }
footer.comment .file { margin:5px 10px; }
footer.comment .comment-edit {margin:5px 5px; }
footer.comment .comment-edit .left { display:table-cell; vertical-align:top; }
footer.comment .comment-edit .right { display:table-cell; vertical-align:top; }
/*footer.comment .comment-edit .search-box { border-radius:3px; background-color:#fff; padding:5px; }*/
footer.comment .comment-edit .search-box textarea { border:1px solid #ddd; width:310px; height:40px; padding:5px 0px 5px 5px;  border-radius:3px; margin-top:3px;} 
footer.comment .comment-edit button { border-radius:5px; background-color:#00B9FF; width:36px; height:50px; margin-top:4px; margin-left:-5px;}
