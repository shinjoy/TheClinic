/* 由ъ뒪�듃 �럹�씠吏� ******************************/
.contents-list {
	margin:0;
	padding:0;
}
.contents-list li {
	list-style:none;
	width:100%;
	cursor:pointer;
	background-color:#fff;
	border-bottom:1px solid #ddd;
}
.contents-list li .contents {
	margin:10px 0;
	width:100%;
}
.contents-list li:hover {
	background-position:0 -69px;
}
.contents-list li.notice-top {
	background-color:#ffffcb;
}
.contents-list li.notice-top:hover {
	background-position:0 69px;
}

.contents-info {
	float:left;
	width:85%;
}
.contents-title {
	margin:10px 5px 8px 20px;
	font-size:16px;
	font-weight:bold;
}
.contents-title-short {
	margin:10px 5px 5px 15px;
	font-size:16px;
	font-weight:bold;
	width:260px;
	color:#333;
}

.contents-title-short-ellipsis {
	margin:10px 5px 5px 15px;
	font-size:16px;
	font-weight:bold;
	color:#333;
}

.contents-user {
	display:inline-block;
	padding:1px 3px;
	margin:0 10px 0 20px;
	color:#fff;
	/*background-color:#9b7e7e;*/
	background-color:#999;
	-moz-border-radius:3px;
	-webkit-border-radius:3px;
	-khtml-border-radius:3px;		
	border-radius:3px;
}
.contents-meta {
	margin-left:10px
}
.contents-arrow {
	float:left;
	margin:13px 0 0 10px ;
}

.bottom-dotted {
	width:100%;
	padding-bottom:10px;
	border-bottom:1px dotted #555;
}
.contents-contents {
	clear:both;
	padding:20px;
}
.contents-contents img {
	width:100%;
}
.contents-tools {
	margin:20px;
}

.read-more {
	width:100%;
	height:80px;
	padding-top:65px;
	text-align:center;
	background-color:#ccc;
}

